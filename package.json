    "ndforge": "^1.5.3",
    "next": "15.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-toastify": "^11.0.5",
    "sass": "^1.89.1",
    "typesdk": "^2.3.4"
  },
  "devDependencies": {
    "@rapid-d-kit/types": "^0.1.1",
    "@types/jsonwebtoken": "^9.0.9",
