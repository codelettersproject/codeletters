  "scripts": {
    "dev": "clear && next telemetry disable && NEXT_TELEMETRY_DISABLED=1 next dev -p 4001",
    "build": "tsc --noEmit && next telemetry disable && NEXT_TELEMETRY_DISABLED=1 next build",
    "start": "tsc --noEmit && next telemetry disable && NEXT_TELEMETRY_DISABLED=1 next start",
    "lint": "next telemetry disable && NEXT_TELEMETRY_DISABLED=1 next lint",
    "spec": "clear && tsc --noEmit && jest --verbose --runInBand",
    "spec:no-check": "clear && jest --verbose --runInBand"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
